<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <link rel="stylesheet" href="css/style.css">



    <title>Calculated Result</title>
    <style>
        body{
            background-color: #f9f9f9;
        }
        main{
            min-height: 30pc;
        }
        button{
            min-width: 50px;
        }
  
        a:link, a:visited {
            color: white;
            padding: 5px;
            text-align: center;
            display: inline-block;
          }
        a:hover{
            color: yellow;
        }
  
        .info{
            background-color: lightblue;
            border-left: solid rgb(80, 80, 235) 5px;
            color: black;
            padding: 10px;
        }
        .info-warning{
            background-color: #f1f79e;
            border-left: solid rgb(255, 238, 0) 5px;
            margin-top: 50px;
            color: red;
            padding: 10px;
        }
  
        nav{
            background-color: magenta;
        }
        nav:a{
            color: black;
        }
        .footer-txt{
            color: black;
        }
        h4{
            text-align: center;
        }
        .main-heading{
            text-align: center;
            padding-top: 10px;

        }
      </style>
      <!-- adsense -->
    <script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-7490554015855837"
    crossorigin="anonymous"></script>
    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-NG01BSD1ZF"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'G-NG01BSD1ZF');
    </script>
    <%- include ('../layout/meta-tag') %>
  </head>
  <body>

    <%- include ('../layout/header') %>

    <main style="min-height: 30pc;">
        <div class="container">
            <h3 class="main-heading font-effect-fire"><%=headingTitle%></h3>
        </div>

        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-6 p-5">
                    <h4><caption>Input Data</caption></h4>
                    <table class="table table-striped">
                        <thead>
                            <tr>
                            <th scope="col">No.</th>
                            <th scope="col">Field</th>
                            <th scope="col">Value</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% for(let key in input) { %>
                                <tr>
                                    <th scope="row"><%=counter1%></th>
                                    <td><%=key%></td>
                                    <td><%=input[key][0] + input[key][1]%></td>
                                </tr>
                                <% counter1++; %>
                            <% } %>
                        </tbody>
                    </table>
                </div>
                <div class="col-lg-6 p-5">
                    <h4><caption>Calculated Data</caption></h4>
                    <table class="table table-striped">
                        <thead>
                            <tr>
                            <th scope="col">No.</th>
                            <th scope="col">Field</th>
                            <th scope="col">Value</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% for(let key in outcome) { %>
                                <tr>
                                    <th scope="row"><%=counter2%></th>
                                    <td><%=key%></td>
                                    <td><%=outcome[key][0] + outcome[key][1]%></td>
                                </tr>
                                <% counter2++; %>
                            <% } %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </main>


    

    <%- include ('../layout/footer') %>
    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
  </body>
</html>
